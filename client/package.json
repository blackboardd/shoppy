{
  "name": "shoppy",
  "description": "Scandiweb's Junior Developer test.",
  "license": "MIT",
  "author": {
    "name": "blackboardd",
    "email": "brightenqtompkins@gmail.com",
    "url": "https://bitbucket.org/blackboardd"
  },
  "repository": {
    "type": "bitbucket",
    "url": "https://bitbucket.org/blackboardd/shoppy"
  },
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/parser": "^5.16.0",
    "bootstrap": "^5.1.3",
    "gulp-json-modify": "^1.0.2",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-dictionary": "style-dictionary build --config ./style-dictionary/config.json",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "type-check": "tsc --noEmit",
    "precommit": "pretty-quick --staged && npm run lint && npm run type-check",
    "prepare": "cd .. && husky install",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp4-run-sequence": "^1.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "run-sequence": "^2.2.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "semantic-release": "^19.0.2",
    "style-dictionary": "^3.7.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "repositoryUrl": "https://auth@bitbucket.org/blackboardd/shoppy.git",
    "tagFormat": "v${version}",
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  }
}
